local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("TITLE", "BloodTheme")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Comet Snipe", "Activate comet snipe", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NoSenseManiac/CometSnipe/main/source", true))()
end)
